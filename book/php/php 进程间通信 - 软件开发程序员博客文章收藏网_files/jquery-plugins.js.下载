var a=$("#keyword").val();
if($.trim(a)=='' || $.trim(a)=='输入关键词...'){
	;
} else {
	$("#keyword").removeAttr("placeholder");
}

function validate(){
	var a=$("#keyword").val();
	if($.trim(a)=='' || $.trim(a)=='输入关键词...'){
		alert('关键词不能为空！');
		return false;
	}
}

function pagedSearch(index){
    $("#curPage").val(index);
	$("#searchForm").submit();
}

